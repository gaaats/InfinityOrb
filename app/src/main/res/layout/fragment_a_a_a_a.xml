<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/baaack"
    tools:context=".AAAAFragment">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.30" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_10ddd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.10" />


    <ImageView
        android:id="@+id/img_el1"
        android:layout_width="240dp"
        android:layout_height="240dp"
        android:src="@drawable/vh2"
        app:layout_constraintBottom_toTopOf="@+id/img_elfrgtgtgtgt"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_05gt"
        app:layout_constraintTop_toTopOf="@id/guideline_hor_05gt" />

    <TextView
        android:id="@+id/tv_big"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_black"
        android:gravity="center"
        android:text="*x260\nBonus"
        android:textAllCaps="true"
        android:textColor="#D6C1E3"
        android:textSize="21sp"
        app:layout_constraintTop_toBottomOf="@id/img_el1"
        app:layout_constraintEnd_toEndOf="@id/btn_big"
        app:layout_constraintStart_toStartOf="@id/btn_big" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_big"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_black"
        android:text="super\ngame"
        app:layout_constraintBottom_toBottomOf="@+id/img_el1"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_95gtgtt"
        app:layout_constraintStart_toEndOf="@+id/img_el1"
        app:layout_constraintTop_toTopOf="@+id/img_el1" />


    <ImageView
        android:id="@+id/img_elfrgtgtgtgt"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:src="@drawable/vh1"
        app:layout_constraintBottom_toBottomOf="@id/guideline_hor_95gttg"
        app:layout_constraintEnd_toStartOf="@+id/btn_small"
        app:layout_constraintStart_toEndOf="@+id/img_mechanicerfgt"
        app:layout_constraintTop_toBottomOf="@+id/img_el1" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_black"
        android:text="play"
        app:layout_constraintBottom_toBottomOf="@+id/img_elfrgtgtgtgt"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_95gtgtt"
        app:layout_constraintStart_toEndOf="@+id/img_elfrgtgtgtgt"
        app:layout_constraintTop_toTopOf="@+id/img_elfrgtgtgtgt" />

    <TextView
        android:id="@+id/tv_small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_black"
        android:gravity="center"
        android:text="*x14\nBonus"
        android:textAllCaps="true"
        android:textColor="#D6C1E3"
        android:textSize="14sp"
        app:layout_constraintTop_toBottomOf="@id/img_elfrgtgtgtgt"
        app:layout_constraintEnd_toEndOf="@id/btn_small"
        app:layout_constraintStart_toStartOf="@id/btn_small" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_05gt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.20" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_13dd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.13" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_95gttg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_85ddd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.85" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_hor_50fgrr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.50" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vert_95gtgtt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vert_05gt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vert_50gtgtg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.50" />

    <ImageView
        android:id="@+id/img_mechanicerfgt"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:src="@drawable/ggggggt"
        app:layout_constraintBottom_toBottomOf="@+id/img_elfrgtgtgtgt"
        app:layout_constraintEnd_toStartOf="@+id/img_elfrgtgtgtgt"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_05gt"
        app:layout_constraintTop_toTopOf="@+id/img_elfrgtgtgtgt" />


</androidx.constraintlayout.widget.ConstraintLayout>