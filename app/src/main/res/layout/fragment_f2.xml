        app:layout_constraintEnd_toEndOf="@id/guideline_vert_95gtgtt"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_05gt"
        app:layout_constraintTop_toTopOf="@id/guideline_hor_05gt" />

    <TextView
        android:id="@+id/gtgtgtgtgt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/purple_500"
        android:fontFamily="@font/montserrat_black"
        android:padding="5dp"
        android:text="You have just a 60 seconds for this game. Harry up and make all tasks."
        android:textColor="#E6BAEC"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@id/gttgttggt"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_95gtgtt"
        app:layout_constraintStart_toEndOf="@id/gttgttggt"
        app:layout_constraintTop_toTopOf="@id/gttgttggt" />

