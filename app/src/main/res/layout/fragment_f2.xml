        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/purple_500"
        android:fontFamily="@font/montserrat_black"
        android:gravity="center"
        android:padding="5dp"
        android:text="@string/app_name"
        android:textColor="#E6BAEC"
        android:textSize="32sp"
        android:textAllCaps="true"
        app:layout_constraintBottom_toBottomOf="@id/guideline_hor_95gttg"
        app:layout_constraintEnd_toEndOf="@id/guideline_vert_95gtgtt"
        app:layout_constraintStart_toStartOf="@id/guideline_vert_05gt" />


